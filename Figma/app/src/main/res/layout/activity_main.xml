<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView

        android:id="@+id/imageView"
        android:layout_width="182dp"
        android:layout_height="185dp"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="272dp"
        android:background="@drawable/iconlandingpage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/icon2landingpage"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.41"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="169dp"
        android:layout_height="401dp"
        android:layout_marginStart="193dp"
        android:layout_marginTop="66dp"
        android:background="@drawable/icon3landingpage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="188.16dp"
        android:layout_height="205.91dp"
        android:layout_marginStart="276dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/polygon1"
        android:rotation="-13"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:layout_width="183dp"
        android:layout_height="253dp"
        android:layout_marginEnd="276dp"
        android:layout_marginBottom="72dp"
        android:background="@drawable/ellipse10"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:layout_width="235dp"
        android:layout_height="246dp"
        android:layout_marginTop="89dp"
        android:layout_marginEnd="296dp"
        android:background="@drawable/ellipse9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="269dp"
        android:layout_height="74dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="124dp"
        android:fontFamily="@font/saira_bold"
        android:text="KIDS Corner"
        android:textSize="37dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="204dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/saira_bold"
        android:text="v1.0.1"
        android:textColor="@color/grey"
        android:textSize="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>